REACT_APP_API_URL=https://api.example.com

# WebSocket URL for real-time communication
REACT_APP_WEBSOCKET_URL=wss://api.example.com/ws

# Google Analytics tracking ID
REACT_APP_GOOGLE_ANALYTICS_ID=UA-XXXXXXXXX-X

# Comma-separated list of supported social media platforms
REACT_APP_SOCIAL_MEDIA_PLATFORMS=facebook,twitter,linkedin,instagram

# Maximum file upload size in MB
REACT_APP_MAX_UPLOAD_SIZE=100

# Default language for the application
REACT_APP_DEFAULT_LANGUAGE=en

# Comma-separated list of feature flags for enabling/disabling features
REACT_APP_FEATURE_FLAGS=newAnalyticsDashboard,improvedContentGenerator